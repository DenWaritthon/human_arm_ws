        <collision>
            <geometry>
                <mesh filename="../meshes/Link1.STL"/>
            </geometry>
        </collision>
    </link>

    <!-- q3 -->
    <joint name="q3" type="revolute">
        <origin xyz="0.345 0.0 0.045" rpy="0.0 -1.57 3.14"/>
        <parent link="Link1"/>
        <child link="Joint2"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="1000" velocity="0.0"/>
    </joint>

    <!-- Joint2 -->
    <link name="Joint2">
