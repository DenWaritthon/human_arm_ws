    <joint name="q7" type="revolute">
        <origin xyz="0.0 0.045 0.045" rpy="-1.57 1.57 0.0"/>
        <parent link="Joint4"/>
        <child link="Gripper"/>
        <axis xyz="0 0 1"/>
        <limit lower="-3.14" upper="3.14" effort="1000" velocity="0.0"/>
    </joint>

